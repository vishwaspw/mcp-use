{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },
    "airbnb": {
      "command": "npx",
      "args": ["-y", "@openbnb/mcp-server-airbnb"]
    },
    "duckduckgo-search": {
      "command": "npx",
      "args": ["-y", "duckduckgo-mcp-server"]
    },
    "weather": {
      "command": "npx",
      "args": ["-y", "weather-mcp-server"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "github-mcp-server"]
    },
    "pdf-reader": {
      "command": "npx",
      "args": ["-y", "pdf-reader-mcp-server"]
    }
  }
}
